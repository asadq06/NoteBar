{
  "name": "Note Bar",
  "version": "1.0",
  "manifest_version": 2,

  "description": "Allows making having quick access to notes.",

  "browser_action": {
    "default_popup": "popup.html" // optional
  },

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery-3.1.1.min.js", "myScript.js" ]  
    }
  ]

}
